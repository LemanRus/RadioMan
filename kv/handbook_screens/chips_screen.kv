                MDShortenLabel:
                    text: "Серия 133"

            MDIconCard:
                on_release: root.parent.current = "chips_analogs_140_screen"

                ImageButton:
                    source: "media/th_capacitor.png"
                    on_release: root.parent.current = "chips_analogs_140_screen"

